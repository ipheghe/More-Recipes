{
  "name": "MoreRecipes",
  "version": "1.0.0",
  "description": "More-Recipes provides a platform for users to share the awesome and exciting  recipe ideas they have invented or learnt.  Suppose a user comes up with a recipe,  he/she can post it on More-Recipes and  get feedback in form of reviews and votes from other users who explore that recipe. Users can also keep a list of their favorite recipes on the application.",
  "main": ".eslintrc.json",
  "scripts": {
    "wp": "webpack-dev-server --hot --history-api-fallback --open",
    "test": "sequelize db:migrate:undo:all --env=test&&sequelize db:migrate --env=test  && cross-env NODE_ENV=test nyc mocha --timeout 15000 tests",
    "heroku-postbuild": "sequelize db:migrate && npm run build:client:prod",
    "build:client:prod": "webpack -p --config webpack.config.prod.js",
    "migrate": "sequelize db:migrate:undo:all&&sequelize db:migrate",
    "start": "babel-node babel-node app.js",
    "start:dev": "cross-env NODE_ENV=development && nodemon --watch server --exec babel-node app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "shc": "start coverage/lcov-report/index.html",
    "copy:images": "cp assets/images/* client/public/dist",
    "webpack": "webpack-dev-server && npm run copy:images",
    "test:client": "jest .client/__tests__/* --coverage",
    "test:watch": "npm test:client -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipheghe/More-Recipes.git"
  },
  "keywords": [],
  "author": "Ovie Ipheghe",
